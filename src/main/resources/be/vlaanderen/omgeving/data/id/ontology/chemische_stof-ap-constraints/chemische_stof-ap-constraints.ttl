#                skos:definition  "Organic compounds containing the functional group -CO-Cl."@en ;
ons:concept a sh:SPARQLTarget ;
            rdfs:label "Sparql query list of nil"@en ;
            sh:select "SELECT ?this WHERE { ?this a <http://www.w3.org/2004/02/skos/core#Concept>. FILTER NOT EXISTS { ?this a <http://dbpedia.org/ontology/ChemicalSubstance>. } }"^^xsd:string .

ons:ChemicalSubstance_ChemicalCompound_Concept a sh:SPARQLTarget ;
           rdfs:label "Sparql query list of nil"@en ;
           sh:select "SELECT ?this WHERE { ?this a <http://www.w3.org/2004/02/skos/core#Concept> ; a <http://dbpedia.org/ontology/ChemicalSubstance> ; a <http://dbpedia.org/ontology/ChemicalCompound>.  }"^^xsd:string .


